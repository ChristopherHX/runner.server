{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      },
      "HttpsFromPem": {
        "Url": "https://*:5001",
        "Certificate": {
          "Path": "server.cert",
          "KeyPath": "server.key"
        }
      }
    }
  },
  "Runner.Host": {
    "ActionDownloadUrls": [
      {
        "TarbalUrl": "https://api.github.com/repos/{0}/tarball/{1}",
        "ZipbalUrl": "https://api.github.com/repos/{0}/zipball/{1}"
      }
    ]
  }
  
}
